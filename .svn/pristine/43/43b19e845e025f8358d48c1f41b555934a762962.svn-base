$("#open").click(function () {
    $("#open").css("display","none");
    $("#refor").css("display","block");
    $("#ul1").css("display","block");
});
$("#refor").click(function () {
    $("#open").css("display","block");
    $("#open").css("margin-top","0");
    $("#ul1").css("display","none");
});