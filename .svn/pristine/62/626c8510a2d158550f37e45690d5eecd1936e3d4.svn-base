<admintpl file="header" />
</head>
<link rel="stylesheet" type="text/css" href="__PUBLIC__/css/addcourse.css" />
<body>
	<div class="wrap">
		<ul class="nav nav-tabs">
			<li><a href="{:U('h5/h5list')}">{:L('ADMIN_H5_INDEX')}</a></li>
			<li><a href="{:U('h5/addh5')}">{:L('ADMIN_H5_ADD')}</a></li>
			<li class="active"><a href="{:U('h5/edith5')}">{:L('ADMIN_H5_EDIT')}</a></li>
		</ul>
		<form method="post" class="form-horizontal js-ajax-form" action="{:U('h5/edit_post')}">
		<input type="hidden" name="editid" value="{$list.id}"/>
			<fieldset>
				<div class="control-group">
					<label class="control-label">{:L('TITLE')}</label>
					<div class="controls">
						<input type="text" name="title" value="{$list.title}">
					</div>
				</div>
				<div class="control-group">
					<label class="control-label">{:L('CONTENT')}</label>
					<div class="controls">
						<script type="text/plain" id="content" name="textareas">{$list.content}</script>
					</div>
				</div>
				<div class="control-group">
					<label class="control-label">{:L('URL')}</label>
					<div class="controls">
						<input type="text" name="url" value="{$list.url}">
					</div>
				</div>
			</fieldset>
			<div class="form-actions">
				<button type="submit" class="btn btn-primary js-ajax-submit">{:L('EDIT')}</button>
				<a class="btn" href="{:U('h5/h5list')}">{:L('BACK')}</a>
			</div>
		</form>
	</div>
	<script src="__PUBLIC__/js/common.js"></script>
	<script type="text/javascript" src="__PUBLIC__/js/ueditor/ueditor.config.js"></script>
	<script type="text/javascript" src="__PUBLIC__/js/ueditor/ueditor.all.min.js"></script>
</body>
<script type="text/javascript">
var editorURL = GV.DIMAUB;
editorcontent = new baidu.editor.ui.Editor();
editorcontent.render('content');
try {
	editorcontent.sync();
} catch (err) {
}
$('#content').css({'width':'700px','height':'200px'});
</script>
</html>