/* Write your styles */	
/*iscroll  样式*/
html,
body,
.page {
	height: 100%;
}
body {
	-webkit-user-select:none;
    -webkit-text-size-adjust:none;
}
    .main-cont {
      margin: 0;
    }

    .pull-action {
      text-align: center;
      height: 45px;
      line-height: 45px;
      color: #999;
    }

    .pull-action .am-icon-spin {
      display: none;
    }

    .pull-action.loading .am-icon-spin {
      display: block;
    }

    .pull-action.loading .pull-label {
      display: none;
    }
#wrapper {
	position:absolute; z-index:1;
	top:0; bottom:45px; left:-9999px;
	width:100%;
	background:#ebebeb;
	overflow:auto;
}
#scroller {
	position:absolute; z-index:1;
/*	-webkit-touch-callout:none;*/
	-webkit-tap-highlight-color:rgba(0,0,0,0);
	width:100%;
	padding:0;
}

#myFrame {
	position:absolute;
	top:0; left:0;
}
.bbbox{padding:10px 0;text-align:center;display:block;}
.pullDownLabel,.pullUpLabel{margin-left:5px;}

